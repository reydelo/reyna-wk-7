<!DOCTYPE html>
<html>
<head>
  <title>ReynaWk7</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if current_user %>
  Signed in as <%= link_to current_user.user_name, user_path(current_user) %> | <%= link_to "Logout", '/logout' %>
  <% else %>
  <%= link_to 'Login', '/login' %> | <%= link_to 'Signup', '/signup' %>
  <% end %>

  <%= yield %>

</body>
</html>
